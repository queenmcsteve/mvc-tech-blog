<div id="new-post">
  <div class="mb-3">
    <label for="exampleFormControlInput1" class="form-label">Post Title</label>
    <input
      type="text"
      name="post-title"
      class="form-control"
      id="post-title"
    />
  </div>
  <div class="mb-3">
    <label for="exampleFormControlTextarea1" class="form-label">Post Content</label>
    <textarea
      class="form-control"
      id="post-content"
